<% if (isAdmin) { %>
    <table>
        <thead>
            <tr>
                <th>Name</th>
                <th>Email</th>
                <th>API Requests</th>
                <th>Admin Status</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach(user => { %>
                <tr>
                    <td><%= user.username %></td>
                    <td><%= user.email %></td>
                    <td><%= user.apiRequests %></td>
                    <td><%= user.isAdmin %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
<% } %>
