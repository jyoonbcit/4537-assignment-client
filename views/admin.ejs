<%- include('templates/navbar') %>

    <% if (isAdmin) { %>
        <div class="table-responsive">
            <table class="table">
                <thead class="table-dark">
                    <tr>
                        <th scope="col">Name</th>
                        <th scope="col">Email</th>
                        <th scope="col">API Requests</th>
                        <th scope="col">Admin Status</th>
                    </tr>
                </thead>
                <tbody>
                    <% users.forEach(user=> { %>
                        <tr>
                            <td>
                                <%= user.username %>
                            </td>
                            <td>
                                <%= user.email %>
                            </td>
                            <td>
                                <%= user.apiRequests %>
                            </td>
                            <td>
                                <%= user.isAdmin ? 'Yes' : 'No' %>
                            </td>
                        </tr>
                        <% }); %>
                </tbody>
            </table>
        </div>
        <% } %>

            <%- include('templates/footer') %>